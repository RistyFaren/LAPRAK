<html>
    <head>
        
        <title> Konversi Nilai Angka ke Huruf</title>
        <script language="Javascript"> 
            document.write ("Modifikasi terakhir pada : " + document.lastModified)
               
                function konversi(){ /*membuat atribut konversi*/
                    /*deklarasi variabel*/
                    var x = document.output.angka.value
                    if (x <= 40)
                        document.output.huruf.value = "E"
                    else if (41 <=x && 55 >= x)
                        document.output.huruf.value = "D"
                    else if (56 <=x && 60 >=x)
                        document.output.huruf.value = "C"
                    else if (61 <=x && 65 >=x)
                        document.output.huruf.value = "BC"
                    else if (66 <=x && 70 >=x)
                        document.output.huruf.value = "B"
                    else if (71 <=x && 80 >=x)
                        document.output.huruf.value = "AB"
                    else if (81 <=x && 100 >=x)
                        document.output.huruf.value = "A"
                    else
                        window.alert("NULL!")
                }
            </script>
    </head>
    <body>
        <h1>KONVERSI NILAI ANGKA KE HURUF</h1>
        <form action="" name="output">
            Masukkan nilai  : 
            <input type = "text" name = "angka" id ="">
            <br> </br>
            Hasil Konversi Huruf :
            <input type = "text" name = "huruf" id ="">
            <br> </br>
            <input type ="button" name = "button1" value="Konversi" onclick="konversi()">

        </form>
    </body>

</html>
